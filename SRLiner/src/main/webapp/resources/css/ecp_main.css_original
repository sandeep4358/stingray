/* roll over Buttons in Favorite Fields */
.colorFlip { 
    filter: grayscale(1); /* Firefox 3.5+ */
    filter: gray; /* IE5+ */
    -webkit-filter: grayscale(1); /* Webkit Nightlies  Chrome Canary */
    opacity: 0.2;
}

/*  roll over Buttons in Favorite Fields */
.colorFlip:hover { 
    filter: none;
    -webkit-filter: grayscale(0); /* Webkit Nightlies  Chrome Canary */
}

.epAlignLeftTable tbody.ui-datatable-data > tr > td {
    padding: 20px !important;
    vertical-align: top !important;
    text-align: left !important;
}

.ui-widget-header {
    color: black;
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield {
    box-shadow: none;
}
  
.vgm-tabset-header .ui-tabs-nav div em{
   width: 70px;   
   padding: 0.5em 2.5em;
   background: url(../ep_img/ecp_container_16.png) no-repeat scroll 5% 29%;
}

.tnt-reporting-copy{
    padding: 20%;
    display:block; 
    background: url(../ep_img/ecp_icon_copy.png)  no-repeat;
}

.tnt-reporting-schedule{
    padding: 20%;
    display:block; 
    background:url(../ep_img/ecp_icon_scheduler.png) no-repeat;
}

.tnt-reporting-schedule-disable{
    padding: 20%;
    display:block; 
    background:url(../ep_img/ecp_scheduler_disabled.png) no-repeat;
}

.tnt-reporting-delete{
    padding: 20%;
    display:block; 
    background: url(../ep_img/ecp_icon_delete.png) no-repeat;
}

.tnt-reporting-edit{
    padding: 20%;
    display:block; 
    background: url(../ep_img/ecp_icon_edit.png) no-repeat;
}

.tnt-reporting-minus{
    background: url(../ep_img/minus.png) no-repeat;
}

.tnt-reporting-plus{
    background: url(../ep_img/plus.png) no-repeat;
}

.tnt-ui-header .ui-widget-header{
 text-shadow: none;
 alignment-adjust: middle;
 background: #CE131D!important;
}

