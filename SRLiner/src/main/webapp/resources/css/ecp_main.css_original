/*************
button and link button styles
*************/

.epButton,
.epButtonRed {
    border: medium none;
    color: #FFFFFF;
    text-align: left;
    height: 30px;
    padding: 0 15px;
}

input.epButton,
input.epButtonRed {
	min-width: 134px; /* inputs minimum width include padding */
}

a.epButton,
a.epButtonRed {
    line-height: 30px;
	min-width: 84px; /* link minimum width excludes padding, so we have to subtract the padding left and right */
}

a.epButton,
input.epButton {
    background-color: #A8A8A8;
}

a.epButtonRed,
input.epButtonRed {
    background-color: #CE131D;
}

.epButton:hover {
    background-color: #888888;
}

.epButtonRed:hover {
    background-color: #990000;
}

a.epButton:hover, a.epButtonRed:hover {
    text-decoration: none;
}

/* additional modifications for input epButton or epButtonRed */
input.cancel,
input.delete,
input.back,
input.add,
a.cancel,
a.delete,
a.back,
a.add {
	padding: 0 15px 0 35px;
	background-position: left center;
	background-repeat: no-repeat;
}

input.cancel,
input.delete,
a.cancel,
a.delete {
        background-position-x: 8px;
	background-image: url(../ep_img/ecp_icon_close_gif.gif);
}

input.back,
a.back {
	background-image: url(../ep_img/ecp_icon_back.gif);
}

input.add,
a.add {
	background-image: url(../ep_img/ecp_icon_add.gif);
}

input.forward,
a.forward {
	padding: 0 35px 0 15px;
	background-position: right center;
	background-repeat: no-repeat;
	background-image: url(../ep_img/ecp_icon_forward.gif);
}

